[honeybag-config]

domain                    = yourdomain.abc
ip_address                = 127.0.0.1
alert_mode_desktop_ini    = true
alert_mode_url_shortcut   = true
token_length              = 6
token_value               = 
token_description         = this is my first honeybag token
url_shortcut_link         = http://placetherealshortcuturlhere.com
url_shortcut_file_name    = placetherealshortcuturlhere.com
folder_name_in_zip_file   = secretfolder
file_name_final_zip_file  = secret.zip


